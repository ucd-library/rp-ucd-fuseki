<http://www.w3.org/2006/vcard/ns#Nickname>
        a                    owl:Class ;
        rdfs:comment         "Specifies the text corresponding to the nickname of the object the vCard represents" ;
        rdfs:label           "Nickname"@en ;
        rdfs:subClassOf      <http://www.w3.org/2006/vcard/ns#Identification> ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( [ a                   owl:Restriction ;
                                                       owl:onProperty      <http://www.w3.org/2006/vcard/ns#nickName> ;
                                                       owl:someValuesFrom  xsd:string
                                                     ]
                                                     [ a                   owl:Restriction ;
                                                       owl:minCardinality  "1"^^xsd:nonNegativeInteger ;
                                                       owl:onProperty      <http://www.w3.org/2006/vcard/ns#nickName>
                                                     ]
                                                   )
                             ] .

